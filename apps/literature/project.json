{
  "root": "apps/literature",
  "sourceRoot": "apps/literature",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/literature",
        "index": "apps/literature/public/index.html",
        "baseHref": "/",
        "main": "apps/literature/src/main.tsx",
        "polyfills": "apps/literature/src/polyfills.ts",
        "tsConfig": "apps/literature/tsconfig.app.json",
        "assets": [
          "apps/literature/public/favicon.ico",
          "apps/literature/src/assets"
        ],
        "styles": [],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "port": 3000,
        "buildTarget": "literature:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "literature:build:production",
          "hmr": false
        }
      }
    }
  },
  "tags": []
}
