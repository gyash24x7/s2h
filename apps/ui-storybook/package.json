{
  "name": "@s2h/ui-storybook",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:css": "postcss styles/index.css --output .storybook/index.css",
    "watch:css": "postcss styles/index.css --output .storybook/index.css --watch",
