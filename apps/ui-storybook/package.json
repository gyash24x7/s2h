    "dev": "run-p watch:css storybook",
    "build": "pnpm run build:css && build-storybook"
  },
  "dependencies": {
    "@s2h/ui": "workspace:*",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.4",
    "@s2h/config": "workspace:*",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/builder-webpack5": "6.4.19",
    "@storybook/manager-webpack5": "6.4.19",
    "@storybook/react": "6.4.19",
    "@types/node": "17.0.17",
    "@types/react": "17.0.39",
    "autoprefixer": "10.4.2",
    "babel-loader": "8.2.3",
    "postcss": "8.4.6",
    "postcss-cli": "9.1.0",
    "postcss-import": "14.0.2",
    "tailwindcss": "3.0.22",
    "typescript": "4.5.5"
  },
